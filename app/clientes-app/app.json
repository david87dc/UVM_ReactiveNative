{
  "expo": {
    "name": "clientes-app",
    "slug": "clientes-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/clientes-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
     "scheme": "clientesapp",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff",
        "scheme": "clientesapp"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.clientesapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
        [
        "expo-splash-screen",
        {
          "backgroundColor": "#232323",
          "image": "./assets/clientes-splash.png",
          "dark": {
            "image": "./assets/clientes-splash.png",
            "backgroundColor": "#000000"
          },
          "imageWidth": 200
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "14e6e3ca-7f87-4f94-99f7-c101a0103004"
      }
    }
  }
}
